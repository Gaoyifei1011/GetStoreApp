<ContentDialog
    x:Class="GetStoreAppWebView.Views.Dialogs.ProcessFailedDialog"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:rootservice="using:GetStoreAppWebView.Services.Root"
    Title="{x:Bind rootservice:ResourceService.GetLocalized('Dialog/WebViewProcessFailed')}"
    BorderBrush="{ThemeResource ContentDialogBorderThemeBrush}"
    CloseButtonText="{x:Bind rootservice:ResourceService.GetLocalized('Dialog/OK')}"
    Content="{x:Bind rootservice:ResourceService.GetLocalized('Dialog/WebViewProcessFailedContent')}"
    Style="{StaticResource DefaultContentDialogStyle}"
    mc:Ignorable="d" />
